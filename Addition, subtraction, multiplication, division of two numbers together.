v1 = float(input("Enter first number: "))
v2 = float(input("Enter second number: "))
print("Addition")
add = float(v1) + float(v2)
print("Result {0} & {1} is {2}".format(v1,v2,add))
print("Subtraction")
sub = float(v2) - float(v1)
print("Result {0} & {1} is {2}".format(v1,v2,sub))
print("Maltiplication")
mal = float(v1) * float(v2)
print("Result {0} & {1} is {2}".format(v1,v2,mal))
print("Division")
div = float(v2) / float(v1) 
print("Result {0} & {1} is {2}".format(v1,v2,div))
